#: src/text-block.c:118 src/text-block.c:638
#, c-format
msgid "ERROR: Unable to open output file %s: %s"
msgstr "FEHLER: Ausgabedatei %s kann nicht geöffnet werden: %s"

#: src/text-block.c:145
#, c-format
msgid "ERROR: Unable to write to output file %s: %s"
msgstr "FEHLER: Ausgabedatei %s kann nicht geschrieben werden: %s"

#: src/text-block.c:181
#, c-format
msgid "ERROR: Unable to read to insert file %s: %s"
msgstr "FEHLER: Einfügedatei %s kann nicht gelesen werden: %s"

#: src/text-block.c:325
msgid "Usage:"
msgstr "Aufruf:"

#: src/text-block.c:443
msgid "ERROR: Too few arguments for select!"
msgstr "FEHLER: Zu wenige Argumente für Zeilenauswahl!"

#: src/text-block.c:477
msgid "ERROR: Invalid value for enumeration-format!"
msgstr "FEHLER: Ungültiger Wert für das Nummerierungsformat!"

#: src/text-block.c:523
msgid "ERROR: Too few arguments for highlight-params!"
msgstr "FEHLER: Zu wenige Argumente für Highlight-Params!"

#: src/text-block.c:535
#, c-format
msgid "ERROR: Invalid argument %s!"
msgstr "FEHLER: Ungültiges Argument %s!"

#: src/text-block.c:547
msgid "ERROR: Invalid min/max actions settings!"
msgstr "FEHLER: Ungültige min/max -Aktioneneinstellungen!"

#: src/text-block.c:558
msgid ""
"ERROR: Select (--select|-s) and tags (--begin-tag/-b/--end-tag/-e/--tag/-t) "
"are mutually exclusive!"
msgstr ""
"FEHLER: Select (-select | -s) und Tags (--begin-Tag/-b/-end-Tag/-e/-Tag/-t) "
"schließen sich gegenseitig aus!"

#: src/text-block.c:570
msgid ""
"WARNING: Begin/end tags (--begin-tag/-b/--end-tag/-e/--tag/-t) have no "
"effect in Cat, Discard, or Enumerate Mode!"
msgstr ""
"WARNUNG: Beginn/End-Tags (--BEGIN-Tag/-b/-End-Tag/-e/-Tag/-t) haben keinen "
"Einfluss auf Cat, Discard oder Enumerate!"

#: src/text-block.c:580
msgid ""
"WARNING: Identical begin/end tags (--tag/-t) with excluded tags (--exclude-"
"tags/-x) are not useful in Extract, Remove, or Replace Mode!"
msgstr ""
"WARNUNG: Identische Beginn/End-Tags (--Tag/-t) mit ausgeschlossenen Tags (-"
"exklude-Tags/-x) sind nicht sinnvoll für Extract, Remove oder Replace!"

#: src/text-block.c:599
#, c-format
msgid "ERROR: Unable to open input file %s: %s"
msgstr "FEHLER: Eingabedatei %s läßt sich nicht öffnen: %s"

#: src/text-block.c:620
#, c-format
msgid ""
"WARNING: Select range (%lld — %lld) is not useful. Input file too short?"
msgstr ""
"WARNUNG: Zeilenauswahlbereich (%lld — %lld) ist nicht sinnvoll. Eingabedatei "
"zu kurz?"

#: src/text-block.c:627
msgid ""
"ERROR: Select from end of file (negative line number) is only possible with "
"input file!"
msgstr ""
"FEHLER: Zeilenauswahl vom Ende der Datei (negative Zeilennummer) ist nur mit "
"Eingabedatei möglich!"

#: src/text-block.c:653
#, c-format
msgid "ERROR: Unable to open insert file %s: %s"
msgstr "FEHLER: Datei %s läßt sich nicht öffnen: %s"

#: src/text-block.c:664
msgid "ERROR: No insert file provided!"
msgstr "FEHLER: Keine Einfügedatei angegeben!"

#: src/text-block.c:825
#, c-format
msgid "ERROR: Read error: %s"
msgstr "FEHLER: Lesefehler: %s"

#: src/text-block.c:839
msgid "ERROR: Number of actions outside of set limits (--min-actions/-m/--max-actions/-M)!"
msgstr "FEHLER: Anzahl der Aktionen liegt außerhalb der festgelegten Grenzen (--min-actions/-m/--max-actions/-M)!"
