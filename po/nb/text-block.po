#: src/text-block.c:118 src/text-block.c:638
#, c-format
msgid "ERROR: Unable to open output file %s: %s"
msgstr "FEIL: Kan ikke åpne utgangsfil %s: %s"

#: src/text-block.c:145
#, c-format
msgid "ERROR: Unable to write to output file %s: %s"
msgstr "FEIL: Kan ikke skrive til utdata %s: %s"

#: src/text-block.c:181
#, c-format
msgid "ERROR: Unable to read to insert file %s: %s"
msgstr "FEIL: Kan ikke lese for å sette inn fil %s: %s"

#: src/text-block.c:325
msgid "Usage:"
msgstr "Bruk:"

#: src/text-block.c:443
msgid "ERROR: Too few arguments for select!"
msgstr "FEIL: For få argumenter for utvalg!"

#: src/text-block.c:477
msgid "ERROR: Invalid value for enumeration-format!"
msgstr "FEIL: Ugyldig verdi for oppregningsformat!"

#: src/text-block.c:523
msgid "ERROR: Too few arguments for highlight-params!"
msgstr "FEIL: For få argumenter for høydepunkt-params!"

#: src/text-block.c:535
#, c-format
msgid "ERROR: Invalid argument %s!"
msgstr "FEIL: Ugyldige argumenter %s!"

#: src/text-block.c:547
msgid "ERROR: Invalid min/max actions settings!"
msgstr "FEIL: Invalid Min/Max Actions -innstillinger!"

#: src/text-block.c:558
msgid "ERROR: Select (--select|-s) and tags (--begin-tag/-b/--end-tag/-e/--tag/-t) are mutually exclusive!"
msgstr "FEIL: Velg (-Velg | -s) og tagger (--Begin-tag/-b/-end-tag/-e/-tag/-t) er gjensidig utelukkende!"

#: src/text-block.c:570
msgid "WARNING: Begin/end tags (--begin-tag/-b/--end-tag/-e/--tag/-t) have no effect in Cat, Discard, or Enumerate Mode!"
msgstr "ADVARSEL: Begynn/sluttkoder (--begin-tag/-b/--end-tag/-e/--tag/-t) har ingen effekt i katt, kast eller oppregner modus!"

#: src/text-block.c:580
msgid "WARNING: Identical begin/end tags (--tag/-t) with excluded tags (--exclude-tags/-x) are not useful in Extract, Remove, or Replace Mode!"
msgstr "ADVARSEL: Identiske start/end-tags (--tag/-t) med ekskluderte tagger (-exclude-tags/-x) er ikke nyttige for å trekke ut, fjerne eller erstatte modus!"

#: src/text-block.c:599
#, c-format
msgid "ERROR: Unable to open input file %s: %s"
msgstr "FEIL: Kan ikke åpne inngangsfil %s: %s"

#: src/text-block.c:620
#, c-format
msgid "WARNING: Select range (%lld — %lld) is not useful. Input file too short?"
msgstr "ADVARSEL: Velg rekkevidde (%lld — %lld) er ikke nyttig. Input File for kort?"

#: src/text-block.c:627
msgid "ERROR: Select from end of file (negative line number) is only possible with input file!"
msgstr "FEIL: Velg fra slutten av filen (negativ linjenummer) er bare mulig med inndatafil!"

#: src/text-block.c:653
#, c-format
msgid "ERROR: Unable to open insert file %s: %s"
msgstr "FEIL: Kan ikke åpne innsett fil %s: %s"

#: src/text-block.c:664
msgid "ERROR: No insert file provided!"
msgstr "FEIL: Ingen innsatsfil oppgitt!"

#: src/text-block.c:825
#, c-format
msgid "ERROR: Read error: %s"
msgstr "FEIL: Les feil: %s"

#: src/text-block.c:839
msgid "ERROR: Number of actions outside of set limits (--min-actions/-m/--max-actions/-M)!"
msgstr "FEIL: Antall handlinger utenfor faste grenser (--min-actions/-m/--max-actions/-M)!"
